<h1>Listing contents</h1>

<table>
  <thead>
    <tr>
      <th>Shipping</th>
      <th>Faq</th>
      <th>Contact</th>
      <th>Home</th>
      <th>About</th>
      <th>Terms</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contents.each do |content| %>
      <tr>
        <td><%= content.shipping %></td>
        <td><%= content.faq %></td>
        <td><%= content.contact %></td>
        <td><%= content.home %></td>
        <td><%= content.about %></td>
        <td><%= content.terms %></td>
        <td><%= link_to 'Show', content %></td>
        <td><%= link_to 'Edit', edit_content_path(content) %></td>
        <td><%= link_to 'Destroy', content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content', new_content_path %>
